<UserControl
    x:Class="MarketSignals.Desktop.Views.MenuView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MarketSignals.Desktop.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <StackPanel>
            <Button
                Command="{Binding NavigateCommand}"
                CommandParameter="SignalsDashboardView"
                Content="Dashboard" />
            <Button
                Command="{Binding NavigateCommand}"
                CommandParameter="YoutubeConfigurationView"
                Content="Youtube" />
            <Button
                Command="{Binding NavigateCommand}"
                CommandParameter="TwitterConfigurationView"
                Content="Twitter" />
            <Button
                Command="{Binding NavigateCommand}"
                CommandParameter="IndicatorsConfigurationView"
                Content="Indicators" />
        </StackPanel>
    </Grid>
</UserControl>
